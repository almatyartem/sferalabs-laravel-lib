{
    "name": "d3sw/laravel-sfera-library",
    "description": "Common Laravel Sfera library",
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "laravel/framework": "^8",
        "spatie/data-transfer-object": "^2.6"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "src"
        ],
        "psr-4": {
            "LaravelSferaLibrary\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^6.14"
    }
}
